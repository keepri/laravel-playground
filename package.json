{
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "clean": "rm -fr node_modules vendor",
        "clean-install": "npm run clean && npm i & composer i",
        "format": "blade-formatter \"./resources/**/*.blade.php\" -w && rustywind --write .",
