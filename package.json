        "laravel-vite-plugin": "^2.0.1",
        "solid-js": "^1.9.10",
        "tailwindcss": "^4.1.18",
        "vite": "^7.3.0",
        "vite-plugin-solid": "^2.11.10"
    },
    "optionalDependencies": {
        "@rollup/rollup-linux-x64-gnu": "4.9.5",
        "@tailwindcss/oxide-linux-x64-gnu": "^4.1.18",
        "lightningcss-linux-x64-gnu": "^1.30.2"
    },
    "devDependencies": {
        "blade-formatter": "^1.44.2",
        "pest": "^0.0.20150905",
        "rustywind": "^0.24.3"
